{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      ".gitignore",
      ".firebaserc",
      "database.rules.json",
      ".git/**"
    ],
    "headers": [ {
	      "source" : "**/*.@(jpg|jpeg|gif|png|svg)",
	      "headers" : [ {
	        "key" : "Cache-Control",
	        "value" : "max-age=604800"
	      } ]
	    },
	    {
	      "source" : "dist/js/*.js",
	      "headers" : [ {
	        "key" : "Cache-Control",
	        "value" : "max-age=604800"
	      } ]
	    }
    ]
  }
}
